<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/h"
    tools:context=".from_receives">

    <Button
        android:id="@+id/insert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/stylebutton"
        android:text="ບັນທຶກ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.247"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.754" />

    <EditText
        android:id="@+id/date"
        android:drawableLeft="@drawable/ic_date"
        android:layout_width="287dp"
        android:layout_height="45dp"
        android:background="@drawable/styleedittext"
        android:ems="10"
        android:enabled="false"
        android:inputType="number"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.113" />

    <EditText
        android:id="@+id/bprices"
        android:layout_width="287dp"
        android:layout_height="45dp"
        android:background="@drawable/styleedittext"
        android:ems="10"
        android:enabled="false"
        android:hint="ລາຄ່າຊຶ່"
        android:drawableLeft="@drawable/ic_buy"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.419" />

    <EditText
        android:id="@+id/qty"
        android:layout_width="287dp"
        android:layout_height="45dp"
        android:background="@drawable/styleedittext"
        android:ems="10"
        android:hint="ຈຳນວນສີນຄ້າ"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:drawableLeft="@drawable/ic_buy"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.52" />

    <EditText
        android:id="@+id/amonte"
        android:layout_width="287dp"
        android:layout_height="45dp"
        android:background="@drawable/styleedittext"
        android:ems="10"
        android:enabled="false"
        android:hint="ລວມເປັນເງີນທັງໜົດ"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.615" />

    <EditText
        android:id="@+id/proid"
        android:layout_width="287dp"
        android:layout_height="45dp"
        android:background="@drawable/styleedittext"
        android:ems="10"
        android:drawableLeft="@drawable/ic_id"
        android:hint="ລະຫັດປະເພດສິນຄ້າ"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.22" />

    <EditText
        android:id="@+id/proname"
        android:layout_width="287dp"
        android:layout_height="45dp"
        android:background="@drawable/styleedittext"
        android:ems="10"
        android:enabled="false"
        android:hint="ຊຶ່ສິນຄ້າສະແດງ"
        android:drawableLeft="@drawable/ic_pro"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.325" />

    <Button
        android:id="@+id/show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/stylebutton"
        android:text="ສະແດງຂໍ້ມູນ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.752"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.754" />

</androidx.constraintlayout.widget.ConstraintLayout>